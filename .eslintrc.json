{
  "env": {
    "es2021": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb-base",
  "ignorePatterns": ["/dist/*.js"],
  "rules": {
    "no-restricted-syntax": "off",
    "import/no-unresolved": [
       "error",
       {
         "ignore": [
           "@octokit/core",
           "^@octokit/plugin-paginate-graphql"
         ]
       }
    ]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
